{
  "name": "quick_demo",
  "version": "1.0.0",
  "description": "Electron + Vue 3 + TypeScript + Vite + Element Plus",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "chcp 65001 && electron-vite dev",
    "build": "electron-vite build",
    "dist": "electron-vite build && electron-builder"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@icon-park/vue-next": "^1.4.2",
    "@vueuse/core": "^13.7.0",
    "electron-store": "^10.1.0",
    "element-plus": "^2.11.1",
    "pinia": "^2.1.6",
    "socket.io-client": "^4.8.1",
    "ssh2": "^1.17.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-virtual-scroller": "^2.0.0-beta.8"
  },
  "devDependencies": {
    "@types/electron-store": "^1.3.1",
    "@types/node": "^20.4.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.0",
    "cross-env": "^10.0.0",
    "electron": "^37.3.1",
    "electron-builder": "^24.13.3",
    "electron-vite": "^4.0.0",
    "postcss": "^8.5.6",
    "sass": "^1.90.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.1.6",
    "vite": "^7.1.3"
  },
  "author": "Developer",
  "license": "MIT",
  "build": {
    "appId": "com.example.electron-vue-app",
    "productName": "quick_demo",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
